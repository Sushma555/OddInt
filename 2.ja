import Java.util.Scanner;
class OddInt
{
public static void main(String[] args)
 {
Scanner sc = new Scanner(System.in);
system.out.println("Enter the digit");
int n = sc.nextInt();
n = n*2;
for(int i=1; i<=n; i++)
   {
 if(i%2!=0)
    {
     System.out.print(i+" ");
    }
   }
    System.out.println();
 }
}



